{
  "name": "pipit",
  "version": "1.0.1",
  "description": "The universal logger with the pluggable architecture.",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./preprocessor/batchMessages": "./preprocessor/batchMessages.js",
    "./preprocessor/levelCutoff": "./preprocessor/levelCutoff.js",
    "./preprocessor/prependArgs": "./preprocessor/prependArgs.js",
    "./preprocessor/prependLevel": "./preprocessor/prependLevel.js",
    "./preprocessor/prependTimestamp": "./preprocessor/prependTimestamp.js",
    "./preprocessor/sendToSentry": "./preprocessor/sendToSentry.js",
    "./preprocessor/stringifyAsJSON": "./preprocessor/stringifyAsJSON.js",
    "./preprocessor/structurizeArgs": "./preprocessor/structurizeArgs.js",
    "./preprocessor/transformArgs": "./preprocessor/transformArgs.js",
    "./preprocessor/writeToConsole": "./preprocessor/writeToConsole.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.build.json && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf lib docs coverage",
    "test": "vitest run",
    "typecheck": "tsc",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/pipit.git"
  },
  "keywords": [
    "logger"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/pipit/issues"
  },
  "homepage": "https://github.com/smikhalevski/pipit#readme",
  "devDependencies": {
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.16",
    "typedoc-plugin-mdn-links": "^5.1.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "parallel-universe": "^6.1.2"
  }
}
